{
  "name": "graaly-frontend",
  "version": "1.0.0",
  "description": "Graaly Frontend",
  "productName": "Graaly",
  "cordovaId": "graaly.app",
  "author": "Eric Mathieu <eric.mathieu13@gmail.com>, Maxime Pacary <maxime.pacary@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "test": "node --experimental-modules test/index.mjs",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:e2e": "cypress open",
    "test:e2e:CI": "cypress run",
    "start:dev": "quasar dev"
  },
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@quasar/babel-preset-app": "^1.0.2",
    "@quasar/extras": "^1.1.2",
    "@tweenjs/tween.js": "^17.2.0",
    "ajv": "^6.9.1",
    "autoprefixer": "^9.5.0",
    "axios": "^0.18.0",
    "axios-cookiejar-support": "^0.4.2",
    "cordova": "^9.0.0",
    "core-js": "^2.5.7",
    "crypto-js": "^3.1.9-1",
    "es6-promisify": "^6.0.1",
    "js-cookie": "^2.2.0",
    "moment": "^2.22.2",
    "object-hash": "^1.3.1",
    "quasar": "^1.0.0-beta.23",
    "sortablejs": "^1.7.0",
    "three": "^0.101.1",
    "three-gltf-loader": "^1.98.0",
    "tough-cookie": "^3.0.1",
    "vue": "^2.6.10",
    "vue-i18n": "^7.8.1",
    "vue2-google-maps": "^0.10.6",
    "vuelidate": "^0.7.4",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0-beta.24",
    "@quasar/quasar-app-extension-testing": "^1.0.0-rc.9",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^1.0.0-beta.7",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0-beta.24",
    "babel-eslint": "^8.2.1",
    "chalk": "^2.4.2",
    "cypress": "^3.2.0",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.3.0",
    "jest-chain": "^1.1.2",
    "start-server-and-test": "^1.7.13",
    "strip-ansi": "^3.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
