/* iPhone 4/4S */
@media only screen and (max-device-width: 350px) and (max-device-height: 640px) and (-webkit-max-device-pixel-ratio: 2) {
  .bubble-middle {
    font-family: arial;
    font-size: 0.8em;
  }
  .character img {
    max-height: 100px !important;
  }
  .text {
    font-size: 0.8em;
  }
  .answers-text .q-btn {
    font-size: 0.8em !important;
  }
  .typed-code {
    font-size: 0.8em;
    margin: 0 !important;
  }
  .keypad .q-btn {
    height: 10% !important;
    font-size: 1rem !important;
  }
  .inventory-btn {
    font-size: 0.6em;
    bottom: 0px !important;
    z-index: 5000;
    background-color: #000;
    width: 100%;
    margin: 0 !important;
  }
  .inventory-btn .q-btn {
    font-size: 7pt;
    min-height: 1em;
    margin: 0px !important;
  }
  .code-image .q-btn {
    font-size: 10pt;
  }
  .code-image .text-grey {
    font-size: 0.8em;
  }
}

